{
  "name": "vyuldashev/cards",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Vladimir Yuldashev",
      "email": "misterio92@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-mbstring": "*",
    "spatie/regex": "^1.2",
    "nesbot/carbon": "^1.22",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3",
    "fzaninotto/faker": "^1.7",
    "larapack/dd": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Vyuldashev\\Cards\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vyuldashev\\Cards\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
